#include <stdio.h>

void main(){
    
    int myArray[] = {13, 14, 8, 40, 32, 40};
    
    int sum;
    printf("Enter the sum: ");
    scanf("%d", &sum);
    
    int arrLength = sizeof(myArray)/sizeof(myArray[0]);
    
    int count = 0;
    for (int i=0; i<arrLength; i++)
    {
        for (int j=0; j<arrLength; j++)
        {
            int sumTwo = myArray[i] + myArray[j];
            if (sumTwo == sum)
            {
                count ++;
            }
        }
    }
    if (count == 0)
    {
        printf("No two elements in array has sum %d", sum);
    }
    else
    {
        printf("The sum exists in the array");
    }
}
