// check if the number is prime or not

#include <stdio.h>

void main(){
    int myNum; 
    printf("Enter the number you want to check prime or not: ");
    scanf("%d", &myNum);
    
    int count = 0;
    for (int i=2; i<myNum; i++)
    {
        if (myNum%i == 0)
        {
            count ++;
        }
    }
    if (count == 0)
    {
        printf("the number is prime");
    }
    else
    {
        printf("the number is composite");
    }
}
