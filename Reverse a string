#include <stdio.h>
#include <string.h>

void main()
{
    char input[20];
    printf("Enter the element to input: ");
    scanf("%s", &input);
    
    int length = strlen(input);
    char revstring[length];
    
    for (int i=0; i<length; i++)
    {
        revstring[i] = input[length-i-1];
    }
    printf("%s", revstring);
    
}
