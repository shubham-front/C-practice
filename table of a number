#include <stdio.h>

void main() {
    // to print the table of given number
    
    int mynum;
    
    printf("type a number to print the table:  \n");
    scanf("%d", &mynum);
    
    printf("The table of %d is as follows:\n", mynum);
    for (int i=1; i<=10; i++)
    {
        printf("%d x %d = %d\n", mynum, i, (mynum*i));
    }
}
