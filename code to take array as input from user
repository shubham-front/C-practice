#include <stdio.h>

// code to take array as input from the user

int main(){
    int n;
    
    printf("enter the number of elements:");
    scanf("%d", &n);
    
    int myArray[n];
    
    printf("Enter %d elements:\n", n);
    for(int i=0; i<n; i++)
    {
        scanf("%d", &myArray[i]);
    }
    printf("you entered: ");
    for(int i=0; i<n; i++)
    {
        printf("%d ", myArray[i]);
    }
}
